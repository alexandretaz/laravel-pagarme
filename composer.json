{
  "name": "alexandretaz/laravel-pagarme",
  "description": "Integration of Pagar.me library with Laravel 5",
  "keywords": ["pagarme", "gateway", "payment", "laravel", "checkout", "intermediary","pagar.me"],
  "type": "libraries",
  "minimum-stability": "dev",
  "license": "MIT",
  "homepage": "https://github.com/filpgame/laravel-pagarme",
  "authors": [
    {
      "name": "Alexandre dos Santos Andrade",
      "email": "alexandretaz@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/config": "5.*",
    "illuminate/support": "5.*",
    "pagarme/pagarme-php": "^1.1",
    "mockery/mockery": "^0.9.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "Alexandretaz\\Pagarme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alexandretaz\\Pagarme\\Tests\\": "tests/"
    }
  },
  "branch-alias": {
    "dev-master": "1.0-dev"
  }
}
